<ESRI.Configuration xmlns="http://schemas.esri.com/Desktop/AddIns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>RS_Tools</Name>
  <AddInID>{b0c70fb1-f041-4b73-8a24-78d7befa0ab9}</AddInID>
  <Description>A collection of tools to enhance RS User user performance. Report bugs ctstayte@gmail.com</Description>
  <Version>1.5.3</Version>
  <Image>Images\RS_Tools.png</Image>
  <Author>ChrisStayte</Author>
  <Company>Woolpert</Company>
  <Date>1/29/2016</Date>
  <Targets>
    <Target name="Desktop" version="10.5" />
  </Targets>
  <AddIn language="CLR" library="RS_Tools.dll" namespace="RS_Tools">
    <ArcMap>
      <Commands>
        <Button id="Woolpert_RS_Tools_HydroTrackerUpdater" class="RS_Tools.ToolCollection.Lidar.HydroTrackerUpdater_Button" message="Will update the attributes for complRSion status of the hydro tile layout: Creator, and QC" caption="Hydro Tracker Updater" tip="Hydro Update Tracker" category="RS Tools" image="Images\HydroTrackerUpdater.png" />
        <Button id="Woolpert_RS_Tools_CleaningTrackerUpdater" class="RS_Tools.ToolCollection.Lidar.CleaningTrackerUpdater_Button" message="Will update the attributes for complRSion status of lidar cleaning tyle layout" caption="Cleaning Tracker Updater" tip="Lidar Cleaning Update Tracker" category="RS Tools" image="Images\CleaningTrackerUpdater.png" />
        <Button id="Woolpert_RS_Tools_TileLasLoaderSettings_Button" class="RS_Tools.ToolCollection.TileLasLoader.TileLasLoaderSettings_Button" message="Will allow access to the Settings form for the Tile LAS Loader Tool " caption="Tile LAS Loader Settings" tip="Configuration for Tile LAS Loader Tool" category="RS Tools" image="Images\TileLasLoaderSettings_Button.png" />
        <Tool id="Woolpert_RS_Tools_TileLasLoader_Tool" class="RS_Tools.ToolCollection.TileLasLoader.TileLasLoader_Tool" message="Will load LAS file base on selected location. Needs Tile Index and LAS names in attribute field." caption="Tile LAS Loader" tip="Load LAS " category="RS Tools" image="Images\TileLasLoader_Tool.png" cursor="Images\TileLasTool.cur" />
        <Button id="Woolpert_RS_Tools_ToggleSelectabilitySettings_Button" class="RS_Tools.ToolCollection.ToggleLayers.Selectable.ToggleSelectabilitySettings_Button" message="Allows the user to map and hot key commands to enable disable layer selectability" caption="Toggle Selector" tip="Toggle layers selectability on and off" category="RS Tools" image="Images\ToggleSelector.png" />
        <Button id="Woolpert_RS_Tools_Selectable_Layer1_Button" class="RS_Tools.ToolCollection.ToggleLayers.Selectable.Layer1_Button" message="Will Toggle Layer 1 Selectability On/Off" caption="Layer 1 Selectable" tip="Toggle Layer Selectability 1 On/Off" category="RS Tools" image="Images\Layer1.png" />
        <Button id="Woolpert_RS_Tools_Selectable_Layer2_Button" class="RS_Tools.ToolCollection.ToggleLayers.Selectable.Layer2_Button" message="Will Toggle Layer 2 Selectability On/Off" caption="Layer 2 Selectable" tip="Toggle Layer Selectability 2 On/Off" category="RS Tools" image="Images\Layer2.png" />
        <Button id="Woolpert_RS_Tools_Selectable_Layer3_Button" class="RS_Tools.ToolCollection.ToggleLayers.Selectable.Layer3_Button" message="Will Toggle Layer 3 Selectability On/Off" caption="Layer 3 Selectable" tip="Toggle Layer Selectability 3 On/Off." category="RS Tools" image="Images\Layer3.png" />
        <Button id="Woolpert_RS_Tools_Selectable_Layer4_Button" class="RS_Tools.ToolCollection.ToggleLayers.Selectable.Layer4_Button" message="Will Toggle Layer 4 Selectability On/Off" caption="Layer 4 Selectable" tip="Toggle Layer Selectability 4 On/Off" category="RS Tools" image="Images\Layer4.png" />
        <Button id="Woolpert_RS_Tools_ToggleVisibilitySettings_Button" class="RS_Tools.ToolCollection.ToggleLayers.Visible.ToggleVisibilitySettings_Button" message="Allows the user to map and hot key commands to enable disable layer visibility" caption="Visibility Settings" tip="Toggle layers visibility on and off" category="RS Tools" image="Images\ToggleVisibilitySettings_Button.png" />
        <Button id="Woolpert_RS_Tools_Visible_Layer1_Button" class="RS_Tools.ToolCollection.ToggleLayers.Visible.Layer1_Button" message="Will Toggle Layer 1 Visibility On/Off" caption="Layer 1 Visible" tip="Toggle Layer Visibility 1 On/Off" category="RS Tools" image="Images\Layer1_Button.png" />
        <Button id="Woolpert_RS_Tools_Visible_Layer2_Button" class="RS_Tools.ToolCollection.ToggleLayers.Visible.Layer2_Button" message="Will Toggle Layer 2 Visibility On/Off" caption="Layer 2 Visible" tip="Toggle Layer Visibility 2 On/Off" category="RS Tools" image="Images\Layer2_Button.png" />
        <Button id="Woolpert_RS_Tools_Visible_Layer3_Button" class="RS_Tools.ToolCollection.ToggleLayers.Visible.Layer3_Button" message="Will Toggle Layer 3 Visibility On/Off" caption="Layer 3 Visible" tip="Toggle Layer Visibility 3 On/Off" category="RS Tools" image="Images\Layer3_Button.png" />
        <Button id="Woolpert_RS_Tools_Visible_Layer4_Button" class="RS_Tools.ToolCollection.ToggleLayers.Visible.Layer4_Button" message="Will Toggle Layer 4 Visibility On/Off" caption="Layer 4 Visible" tip="Toggle Layer Visibility 4 On/Off" category="RS Tools" image="Images\Layer4_Button.png" />
        <Button id="Woolpert_RS_Tools_CollapseRasterLayersTOC_Button" class="RS_Tools.Buttons.CollapseRasterLayersTOC_Button" message="Collapse All Raster Layers In Table Of Contents" caption="Collapse Raster Layers TOC" tip="Collapse All Raster Layers in TOC" category="RS Tools" image="Images\CollapseRasterLayersTOC.png" />
      </Commands>
      <DockableWindows>
        <DockableWindow id="Woolpert_RS_Tools_HydroTrackerUpdate_Dock" class="RS_Tools.DockWins.HydroTrackerUpdate_Dock+AddinImpl" caption="Hydro Tracker Update" image="Images\HydroTrackerUpdateDock.png">
          <InitialPlacement height="250" width="150" state="unpinned" position="float" />
        </DockableWindow>
        <DockableWindow id="Woolpert_RS_Tools_CleaningTrackerUpdate_Dock" class="RS_Tools.DockWins.CleaningTrackerUpdate_Dock+AddinImpl" caption="Cleaning Tracker Update" image="Images\CleaningTrackerUpdateDock.png">
          <InitialPlacement height="250" width="150" state="unpinned" position="float" />
        </DockableWindow>
        <DockableWindow id="RS_Tools_DockWins_ToggleSelectability_Dock" class="RS_Tools.DockWins.ToggleSelectability_Dock+AddinImpl" caption="Toggle Selectability" image="Images\ToggleSelectorDock.png">
          <InitialPlacement height="250" width="200" state="unpinned" position="float" />
        </DockableWindow>
        <DockableWindow id="Woolpert_RS_Tools_ToggleVisibility_Dock" class="RS_Tools.DockWins.ToggleVisibility_Dock+AddinImpl" caption="Toggle Visibility" image="Images\ToggleVisibility_Dock.png">
          <InitialPlacement height="300" width="300" state="pinned" position="float" />
        </DockableWindow>
      </DockableWindows>
      <Toolbars>
        <Toolbar id="Woolpert_RS_Tools_RS_Tracker_Toolbar" caption="RS Lidar Tracker Toolbar" showInitially="false">
          <Items>
            <Button refID="Woolpert_RS_Tools_HydroTrackerUpdater" />
            <Button refID="Woolpert_RS_Tools_CleaningTrackerUpdater" />
          </Items>
        </Toolbar>
        <Toolbar id="Woolpert_RS_Tools_RS_Tile_Las" caption="RS Tile Las Toolbar" showInitially="false">
          <Items>
            <Item refID="Woolpert_RS_Tools_TileLasLoaderSettings_Button" />
            <Item refID="Woolpert_RS_Tools_TileLasLoader_Tool" />
          </Items>
        </Toolbar>
        <Toolbar id="Woolpert_RS_Tools_RS_Selectability_Toggler" caption="RS Selectability Toggler" showInitially="false">
          <Items>
            <Item refID="Woolpert_RS_Tools_ToggleSelectabilitySettings_Button" />
            <Item refID="Woolpert_RS_Tools_Selectable_Layer1_Button" separator="true" />
            <Button refID="Woolpert_RS_Tools_Selectable_Layer2_Button" />
            <Button refID="Woolpert_RS_Tools_Selectable_Layer3_Button" />
            <Button refID="Woolpert_RS_Tools_Selectable_Layer4_Button" />
          </Items>
        </Toolbar>
        <Toolbar id="Woolpert_RS_Tools_RS_Visibility_Toggler" caption="RS Visibility Toggler" showInitially="false">
          <Items>
            <Button refID="Woolpert_RS_Tools_ToggleVisibilitySettings_Button" />
            <Button refID="Woolpert_RS_Tools_Visible_Layer1_Button" separator="true" />
            <Button refID="Woolpert_RS_Tools_Visible_Layer2_Button" />
            <Button refID="Woolpert_RS_Tools_Visible_Layer3_Button" />
            <Button refID="Woolpert_RS_Tools_Visible_Layer4_Button" />
          </Items>
        </Toolbar>
        <Toolbar id="Woolpert_RS_Tools_RS_Miscellaneous" caption="RS Miscellaneous" showInitially="false">
          <Items>
            <Button refID="Woolpert_RS_Tools_CollapseRasterLayersTOC_Button" />
          </Items>
        </Toolbar>
      </Toolbars>
    </ArcMap>
  </AddIn>
</ESRI.Configuration>